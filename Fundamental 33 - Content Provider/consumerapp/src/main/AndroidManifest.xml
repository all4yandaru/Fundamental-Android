<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.lib1">
    <!--TODO 6 Consumer: tambahin permission-->
    <uses-permission android:name="com.project.mynotesapp.READ_DATABASE" />
    <uses-permission android:name="com.project.mynotesapp.WRITE_DATABASE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyNotesApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NoteAddUpdateActivity" />
    </application>

    <!--TODO 7 Consumer: tambahin queries-->
    <queries>
        <package android:name="com.project.mynotesapp" />
    </queries>

</manifest>